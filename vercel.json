{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://jobspeak-backend-production.up.railway.app/api/:path*" },
    { "source": "/ai/:path*", "destination": "https://jobspeak-backend-production.up.railway.app/ai/:path*" },
    { "source": "/voice/:path*", "destination": "https://jobspeak-backend-production.up.railway.app/voice/:path*" },
    { "source": "/resume/:path*", "destination": "https://jobspeak-backend-production.up.railway.app/resume/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
